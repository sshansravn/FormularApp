<form action="/ModtagFormular" method="post" name="Kontaktformular">
    <div class="form-group">
        <label for="navn">Navn</label>
        <input type="text" name="navn" id="navn" placeholder="Tast navn">
    </div>
    <div class="form-group">
       <label for="email">Email </label>
        <input type="email" name="email" id="email" placeholder="Tast email">
    </div>
    <div class="form-group">
        <label for="telefon">Telefon </label>
        <input type="text" id="telefonnummer" name="telefon" placeholder="Tast telefonnummer">
    </div>


    <button type="submit" class="btn btn-primary">Send formular</button>

</form>